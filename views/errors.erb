<div class='jumbotron'>
  <h1>Shoe Stores <br> Supplies Sold</h1>
</div>
<div class="row">
  <div class="col-md-4">
    <div class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title">Oops, there were some errors: </h3>
      </div>
      <div class="panel-body">
        <% if @brand %>
          <ul>
            <% @brand.errors().full_messages().each() do |message| %>
            <li><%= message %></li>
            <% end %>
          </ul>
        <% end %>
        <% if @store %>
          <ul>
            <% @store.errors().full_messages().each() do |message| %>
            <li><%= message %></li>
            <% end %>
          </ul>
        <% end %>
      </div>
    </div>
  </div>
</div>
<a class="btn btn-danger btn-lg" href='/' role="button">Return to Homepage</a>
